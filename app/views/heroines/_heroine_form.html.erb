<%= form_for @heroine do |h| %>
<%= h.label :name %><br>
<%= h.text_field :name %><br>
  <% if @heroine.errors.any? %>
    <div class="error_list">
      <% @heroine.errors.full_messages.each do |x| %>
      <%= x %>
      <% end %>
    </div>
  <% end %><br>
<%= h.label :super_name %><br>
<%= h.text_field :super_name %><br>
<%= h.label "Power" %><br>
<%= collection_select(:heroine, :power_id, Power.all, :id, :name)%><br>
<br><%= h.submit %>
<% end %>
